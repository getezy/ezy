{
  "name": "simple-service",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "proto": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=env=node,outputClientImpl=false,outputServices=grpc-js --ts_proto_out=./src/generated ./proto/simple.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.6.8"
  },
  "devDependencies": {
    "ts-node": "10.9.1",
    "ts-proto": "1.121.1",
    "typescript": "4.7.4"
  }
}
