{
  "name": "ezy",
  "version": "1.0.0-beta.15",
  "description": "GUI client for gRPC/gRPC-Web",
  "main": "./out/main/index.js",
  "author": "Alexey Vasyukov (@notmedia)",
  "homepage": "https://www.getezy.dev",
  "license": "MPL-2.0",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:app": "tsc --noEmit -p tsconfig.app.json --composite false",
    "typecheck:splash-screen": "tsc --noEmit -p tsconfig.splash-screen.json --composite false",
    "typecheck:migrations": "tsc --noEmit -p migrations/tsconfig.json --composite false",
    "typecheck": "npm run typecheck:node &&  npm run typecheck:app && npm run typecheck:splash-screen && npm run typecheck:migrations",
    "test": "npm run test:core",
    "test:core": "jest --config jest.config.core.js",
    "test:core:cov": "CI=true jest --config jest.config.core.js",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -w",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "electron-vite build && electron-builder --win --config electron-builder.config.js -p never",
    "build:mac": "electron-vite build && electron-builder --mac --config electron-builder.config.js -p never",
    "build:linux": "electron-vite build && electron-builder --linux --config electron-builder.config.js -p never",
    "link-database": "./scripts/link-database.sh",
    "migrate:build": "cd migrations && rimraf dist && tsc -p ./tsconfig.build.json",
    "migrate:latest": "TZ=UTC knex migrate:latest --knexfile ./migrations/knexfile.ts",
    "migrate:make": "TZ=UTC knex migrate:make -x ts --knexfile ./migrations/knexfile.ts --migrations-directory ./src",
    "migrate:rollback": "TZ=UTC knex migrate:rollback --knexfile ./migrations/knexfile.ts"
  },
  "dependencies": {
    "@automapper/classes": "8.7.7",
    "@automapper/core": "8.7.7",
    "@automapper/mikro": "8.7.7",
    "@codemirror/commands": "6.1.3",
    "@codemirror/lang-json": "6.0.1",
    "@codemirror/language": "6.4.0",
    "@codemirror/state": "6.2.0",
    "@codemirror/view": "6.7.3",
    "@dnd-kit/core": "6.0.7",
    "@dnd-kit/modifiers": "6.0.1",
    "@dnd-kit/sortable": "7.0.2",
    "@electron-toolkit/preload": "1.0.3",
    "@electron-toolkit/utils": "1.0.2",
    "@fortawesome/free-solid-svg-icons": "6.2.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@getezy/kbar": "0.1.0-beta.39",
    "@grpc/grpc-js": "1.8.4",
    "@grpc/proto-loader": "0.7.4",
    "@improbable-eng/grpc-web": "0.15.0",
    "@lezer/highlight": "1.1.3",
    "@mikro-orm/core": "5.6.6",
    "@mikro-orm/sqlite": "5.6.6",
    "@nextui-org/react": "1.0.0-beta.12",
    "@uiw/react-codemirror": "4.19.6",
    "chroma-js": "2.4.2",
    "electron-store": "8.1.0",
    "electron-updater": "5.3.0",
    "immer": "9.0.17",
    "knex": "2.4.0",
    "lodash": "4.17.21",
    "nanoid": "4.0.0",
    "re-resizable": "6.9.9",
    "react": "18.2.0",
    "react-color": "2.19.3",
    "react-dom": "18.2.0",
    "react-hook-form": "7.42.0",
    "react-select": "5.7.0",
    "react-toastify": "9.1.1",
    "react-use": "17.4.0",
    "type-fest": "3.5.1",
    "uuid": "9.0.0",
    "zustand": "4.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@electron-toolkit/tsconfig": "1.0.1",
    "@electron/notarize": "1.2.3",
    "@release-it/conventional-changelog": "5.1.1",
    "@swc/core": "1.3.26",
    "@types/chroma-js": "2.1.4",
    "@types/jest": "29.2.5",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-color": "3.0.6",
    "@types/react-dom": "18.0.10",
    "@types/uuid": "9.0.0",
    "@vitejs/plugin-react-swc": "3.1.0-beta.2",
    "electron": "22.0.2",
    "electron-builder": "23.6.0",
    "electron-vite": "1.0.17",
    "eslint": "8.31.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.4",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "prettier": "2.8.2",
    "release-it": "15.6.0",
    "ts-jest": "29.0.4",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vite-plugin-run": "0.2.1",
    "vite-plugin-static-copy": "0.13.0"
  }
}
